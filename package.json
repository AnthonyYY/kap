    "babel-preset-stage-3": "6.24.1",
    "cssnano": "4.0.3",
    "del": "3.0.0",
    "devtron": "1.4.0",
    "electron-builder": "20.24.4",
    "electron-connect": "0.6.3",
    "electron-react-devtools": "0.5.3",
    "gulp": "4.0.0",
    "gulp-babel": "7.0.1",
    "gulp-filter": "5.1.0",
    "gulp-inject-svg": "0.1.18",
    "gulp-postcss": "7.0.1",
    "gulp-pug": "4.0.1",
    "gulp-stylelint": "7.0.0",
